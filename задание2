my_list = [1, 2, 3]
print("Исходный список:", my_list)

my_list[0] = 100
print("Список после изменения:", my_list)

my_tuple = (1, 2, 3)
print("Исходный кортеж:", my_tuple)

try:
    my_tuple[0] = 100
except TypeError as e:
    print("Ошибка:", e)

    my_string = "cat"
print("Исходная строка:", my_string)

try:
    my_string[0] = 'b'
except TypeError as e:
    print("Ошибка:", e)

new_string = "b" + my_string[1:]
print("Новая строка:", new_string)
